class Solution {
    public int countElements(int[] nums, int k) {
        Arrays.sort(nums);
        int n=nums.length;
        int ans=0;
        int i=0;
        while(i<n){
            int j=i;
            while(j<n && nums[i]==nums[j])j++;
            int freq=j-i;
            int greater=n-j;
            if(greater>=k)
                ans+=freq;
            

           i=j; 
        }
return ans;
    }
}
