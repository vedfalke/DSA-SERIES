Brute force approach


class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        int count=0;
        ListNode temp=head;
        while(temp!=null){
            count++;
            temp=temp.next;
        }
        if(n==count){
            return head.next;
        }
    
    int length=count-n;
    temp=head;
    for(int i=1;i<length;i++){
            temp=temp.next;
    }
     temp.next=temp.next.next;
     return head;
    }

}
